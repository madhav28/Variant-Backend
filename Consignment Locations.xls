<script language="javascript">
	$(document).ready(function(){
	if('True' == 'True'){
		if(0 == 0)
			$(".listTitleRight").append("<span style='float:right'><button class='btn btn-inverse' onclick='Email();' id='EmailConsignmentInventory'>Email Selected Consignment Reports</button></span>");
		else
			$(".listTitleRight").append("");
	}
		if('' == "All")
			$("#dbStatusBoth").prop("checked", true);
		else if('' == "Inactive")
			$("#dbStatusInactive").prop("checked", true);
		else
			$("#dbStatusActive").prop("checked", true);
});

$("input[type=\"radio\"]").click(function(){
	var q1;
	var tab = 0
if ($('input:radio[name=q1]:checked').val() == "All")
	q1 = "All";
else if ($('input:radio[name=q1]:checked').val() == "Inactive")
	q1 = "Inactive";
else
	q1 = "Active";
location.href = "listConsignmentLocations.aspx?tab=" + tab + "&q1=" + q1
});

function SelectAll(obj)
{
 if(obj.checked)
 {
	 for (var i=0; i < 19 ; i++)
	 {
		if(!$("#selectConsignment_"+i).prop('hide'))
			document.getElementById('selectConsignment_'+i).checked=true;  
	 }
 }
 else
	UnSelectAll(); 
}
function UnSelectAll()
{
 for (var i=0; i < 19 ; i++)
 {
	document.getElementById('selectConsignment_'+i).checked=false;  
 }
 
}
function Email()
{
	
	var id = 0;
	var NoemailAddressCount = 0
	$('.linecheck').each(function(){
		if($(this).attr('checked'))
		{
			id = id + "," + $(this).val();
			var tr = tr = $(this).closest('tr');
		}
	});		
	if ( id == "0"){
		alert("Please select atleast one Consignment Customer to email.")
	}
	else{
		var ConfirmSend;
		/*if(NoemailAddressCount > "0"){
			ConfirmSend=confirm("No Email address for "+ NoemailAddressCount + " Customer. Are you still want to continue?");
		}
		else{*/
			ConfirmSend = confirm('Are you sure to send mail?');
		//}
		if(ConfirmSend){
			$('#EmailConsignmentInventory').attr('Disabled', true);
			$('#ajax-sending').center().show();
			$.post("listConsignmentLocations.aspx?act=SendEmail&IDs="+ id +"&z=" + new Date().getTime(),function(data){
			alert(data);
			$('#ajax-sending').center().hide();
			location.href= location.href;
			})
		}
	}
	return false;

}
</script>
